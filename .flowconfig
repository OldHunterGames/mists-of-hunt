[ignore]

[include]

[libs]

[lints]

[options]
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.less
module.system.node.resolve_dirname=src/renderer/components
module.system.node.resolve_dirname=src/renderer
module.system.node.resolve_dirname=node_modules
module.name_mapper='^@game/\(.*\)$' -> '<PROJECT_ROOT>/src/game/\1'
module.name_mapper.extension='css' -> 'css-module'
module.name_mapper.extension='less' -> 'css-module'
esproposal.class_static_fields=enable
esproposal.class_instance_fields=enable

[strict]
